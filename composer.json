{
    "name": "WeiKai/ideaBuy",
    "description": "The ideaBuy coreApi",
    "keywords": ["framework", "laravel", "ideaBuy"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aliyuncs/oss-sdk-php": "^2.2",
        "barryvdh/laravel-debugbar": "^2.4",
        "dingo/api": "1.0.*@dev",
        "jpush/jpush": "^3.5",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "lokielse/omnipay-alipay": "^2.3",
        "lokielse/omnipay-unionpay": "dev-master",
        "nwidart/laravel-modules": "^1.26",
        "overtrue/easy-sms": "^0.0.9",
        "overtrue/laravel-wechat": "~3.0",
        "predis/predis": "1.0.*",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weibo": "^3.0",
        "socialiteproviders/weixin": "^4.0",
        "tymon/jwt-auth": "0.5.*",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Libraries\\": "Libraries/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
